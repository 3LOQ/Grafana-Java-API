{
    "Version": "0.1",
    "Authentication": "oAuth",
    "Link": "http://docs.grafana.org/http_api/dashboard/",
    
    
    "DashboardByUid": {
        "Methode": "GET",
        "URL": "/api/dashboards/uid/:uid",
        "Parameter": ":uid",
        "Response": "/responses/Dashboard/Dashboard.json"
    },
    "DashboardHome": {
        "Methode": "GET",
        "URL": "/api/dashboards/home",
        "Response": "/responses/Dashboard/GetHomeDashboard.json"
    },
    "DashboardTags": {
        "Methode": "GET",
        "URL": "/api/dashboards/tags",
        "Response": "/responses/Dashboard/GetDashboardTag.json"
    },
    "CreateUpdateDashboard": {
        "Methode": "POST",
        "URL": "/api/dashboards/db",
        "Response": "/responses/Dashboard/CreateUpdateDashboard.json",
        "Template": "/templates/Dashboard/CreateUpdateDashboard.json"
    },
    "DeleteDashboardByUid": {
        "Methode": "DELETE",
        "URL": "/api/dashboards/uid/:uid",
        "Parameter": ":uid",
        "Response": "/responses/Dashboard/DeleteByID.json"
    }
}
