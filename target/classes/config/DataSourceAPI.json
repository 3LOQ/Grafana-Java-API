{
  "Version": "0.1",
  "Authentication": "oAuth",
  "Link": "http://docs.grafana.org/http_api/dashboard/",


  "DataSourceByUid": {
    "Methode": "GET",
    "URL": "/api/datasources/:datasourceId",
    "Parameter": ":datasourceId",
    "Response": "/responses/DataSource/DataSourceId.json"
  },
  "DataSourceByName": {
    "Methode": "GET",
    "URL": "/api/datasources/name/:name",
    "Parameter": ":name",
    "Response": "/responses/DataSource/DataSourceName.json"
  },
  "DataSourceIdByName": {
    "Methode": "GET",
    "URL": "/api/datasources/id/:name",
    "Parameter": ":name",
    "Response": "/responses/DataSource/DataSourceIdName.json"
  },
  "CreateDataSource": {
    "Methode": "POST",
    "URL": "/api/datasources",
    "Response": "/responses/DataSource/CreateDataSource.json",
    "Template": "/templates/DataSource/CreateDataSource.json"
  },
  "UpdateDataSource": {
    "Methode": "PUT",
    "URL": "/api/datasources/:datasourceId",
    "Parameter": ":datasourceId",
    "Response": "/responses/DataSource/UpdateDataSource.json",
    "Template": "/templates/DataSource/UpdateDataSource.json"
  },
  "DeleteDataSourceByUid": {
    "Methode": "DELETE",
    "URL": "/api/datasources/:datasourceId",
    "Parameter": ":datasourceId",
    "Response": "/responses/DataSource/DeleteDataSourceByID.json"
  }
}
