{
    "Version": "0.1",
    "Authentication": "oAuth",
    "Link": "http://docs.grafana.org/http_api/org/",
    
    
    "CurrOrgList": {
        "Methode": "GET",
        "URL": "/api/org",
        "Response": "/responses/Organisation.json"
    },
    "CurrOrgUsers": {
        "Methode": "GET",
        "URL": "/api/org/users",
        "Response": "/responses/OrganisationUsers.json"
    },
    "UpdateCurrOrg": {
        "Methode": "PUT",
        "URL": "/api/org",
        "Response": "/responses/Message.json",
        "Template": "/templates/Organisation/CreateUpdateOrg.json"
    },
    "UpdateCurrOrgUserByID": {
        "Methode": "PATCH",
        "URL": "/api/org/users/:userId",
        "Parameter": ":userId",
        "Response": "/responses/Message.json",
        "Template": "/templates/Organisation/NewUpdateOrgUserByID.json"
    },
    "AddOrgCurrUser": {
        "Methode": "POST",
        "URL": "/api/org/users",
        "Response": "/responses/Message.json",
        "Template": "/templates/Organisation/AddOrgUser.json"
    },
    "DeleteCurrOrgUserByID": {
        "Methode": "DELETE",
        "URL": "/api/org/users/:userId",
        "Parameter": ":userId",
        "Response": "/responses/Message.json"
    }
}
