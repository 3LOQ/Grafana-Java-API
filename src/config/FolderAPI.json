{
    "Version": "0.1",
    "Authentication": "oAuth",
    "Link": "http://docs.grafana.org/http_api/org/",
    
    
    "FolderList": {
        "Methode": "GET",
        "URL": "/api/folders",
        "Response": "/responses/Folder/Folder.json"
    },
    "FolderByUid": {
        "Methode": "GET",
        "URL": "/api/folders/:uid",
        "Parameter": ":uid",
        "Response": "/responses/Folder/Folder.json"
    },
    "FolderByID": {
        "Methode": "GET",
        "URL": "/api/folders/id/:id",
        "Parameter": ":id",
        "Response": "/responses/Folder/FolderObject.json"
    },
    "CreateFolder": {
        "Methode": "POST",
        "URL": "/api/folders",
        "Response": "/responses/Folder/FolderObject.json",
        "Template": "/templates/Folder/CreateFolder.json"
    },
    "UpdateFolder": {
        "Methode": "PUT",
        "URL": "/api/folders/:uid",
        "Parameter": ":uid",
        "Response": "/responses/Folder/FolderObject.json",
        "Template": "/templates/Folder/UpdateFolder.json"
    },
    "DeleteFolderByUid": {
        "Methode": "DELETE",
        "URL": "/api/folders/:uid",
        "Parameter": ":uid",
        "Response": "/responses/Message.json"
    }
}